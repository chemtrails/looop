{
    "manifest_version": 2,
    "name": "Looop",
    "version": "0.2",
    "description": "Loop songs and videos",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "content.css"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "contextMenus",
        "tabs"
    ],
    "icons": {
        "16": "16.png",
        "32": "32.png",
        "64": "64.png",
        "128": "128.png"
    }
}